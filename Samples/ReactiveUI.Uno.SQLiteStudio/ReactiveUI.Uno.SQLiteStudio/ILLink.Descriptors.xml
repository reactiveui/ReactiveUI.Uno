<?xml version="1.0" encoding="utf-8"?>
<linker>
  <!-- Preserve ReactiveUI.Uno hosts used by XamlReader and RoutedViewHost to avoid trimming issues in AOT -->
  <assembly fullname="ReactiveUI.Uno">
    <type fullname="ReactiveUI.Uno.ViewModelViewHost" preserve="all" />
    <type fullname="ReactiveUI.Uno.RoutedViewHost" preserve="all" />
  </assembly>

  <!-- Preserve application Views that can be instantiated from XAML or ViewLocator -->
  <assembly fullname="ReactiveUI.Uno.SQLiteStudio">
    <type fullname="ReactiveUI.Uno.SQLiteStudio.Views.MainView" preserve="all" />
  </assembly>
</linker>
